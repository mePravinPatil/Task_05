const random = ['madam', 12321, 'abcba','acs'];
const newArr = [];

const palindrome = function (abc) {
    
    for(let i=0; i < abc.length; i++) {
        let rev = String(abc[i]).split('').reverse().join('');
         if (rev == abc[i]){
            newArr.push(abc[i]);  
            
        }
       
    }
    console.log(newArr);
}
palindrome(random);
